#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	int edad;
	cout<<"ingresar la edad: ";
	cin>>edad;
	if(edad>=18)
		cout<<"usted puede votar: ";
	else
		cout<<"usted no puede votar: ";
	getch();
}
