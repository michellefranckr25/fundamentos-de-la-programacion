// sistema de ecuaciones.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main ()
{
	float a,b,c,d,e,f,x,y;
	cout<<"ingrese el valor de a: ";
	cin>>a;
	cout<<"ingrese el valor de b: ";
	cin>>b;
	cout<<"ingrese el valor de c: ";
	cin>>c;
	cout<<"ingrese el valor de d: ";
	cin>>d;
	cout<<"ingrese el valor de e: ";
	cin>>e;
	cout<<"ingrese el valor de f: ";
	cin>>f;
	x=(f*b-e*c)/(b*d-e*a);
	cout<<"el valor de x es: "<<x;
	cout<<endl;
	y=(c-a*x)/b;
	cout<<"el valor de y es: "<<y;
getch();
}
